  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": null
    },
    "storageAccountType": {
      "value": "Standard_LRS"
    },
    "location": {
      "value": "[resourceGroup().location]"
    },
    "runtime": {
      "value": "powershell"
    },
    "ClientID": {
      "value": "Provide the client ID"
    },
    "ClientSecret": {
      "value": "Provide the Client Secret"
    },
